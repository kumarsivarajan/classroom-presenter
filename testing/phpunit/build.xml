<project name="phpunitExample" default="build">


 <target name="simpleTest">
   <exec dir=".." executable="phpunit" failonerror="false">
      <arg line="testTesting.php" />
        </exec>
         </target>

 <target name="testLogout">
   <exec dir=".." executable="phpunit" failonerror="false">
      <arg line="testLogout.php" />
        </exec>
         </target>
<target name="testGetAlias">
   <exec dir="." executable="phpunit" failonerror="false">
     <arg line="test-alias.php" />
       </exec>
         </target>
		 
<target name="testSubmitQuestion">
	<exec dir="." executable="phpunit" failonerror="false">
	 <arg line="test-submit-question.php" />
		</exec>
		 </target>
		 
<target name="testSubmitVote">
	<exec dir="." executable="phpunit" failonerror="false">
	 <arg line="test-submit-vote.php" />
		</exec>
		 </target>

<target name="testStudentFeed">
	<exec dir="." executable="phpunit" failonerror="false">
	 <arg line="test-studentfeed.php" />
		</exec>
		 </target>

 <target name="build" depends="simpleTest"/>
 <target name="logout" depends="testLogout"/>
 <target name="alias" depends="testGetAlias"/> 
 <target name="submitQuestion" depends="testSubmitQuestion"/>
 <target name="submitVote" depends="testSubmitVote"/>
 <target name="studentFeed" depends="testStudentFeed"/>

 </project>
